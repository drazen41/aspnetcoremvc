@{ Layout = null; }
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Razor</title>
    <link asp-href-include="lib/bootstrap/css/*.min.css" rel="stylesheet" />
    <script id="jsonData" type="application/json">
        @Json.Serialize(new string[] { "Apple","Orange","Pear"})

    </script>
    <script asp-src-include="lib/jquery/*.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var list = $("#List");
            JSON.parse($("#jsonData").text()).forEach(function (val) {
                console.log("Val: " + val);
                list.append($("<li>").text(val));
            });
        });
    </script>
</head>
<body class="m-1 p-1">
    This is the List View
    @*@Html.Partial("MyPartial")*@
    <partial name="MyStronglyTypedPartial" model="@{ new string[] { "Apple", "Orange", "Pear" };}" />
    <ul id="List"></ul>
</body>
</html>